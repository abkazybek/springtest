        <hierarchicalDatasource id="departmentsDs" class="com.company.thesissummer.entity.ExtDepartment"/>
    </dsContext>
    <layout>
        <hbox id="mainBox">
            <vbox id="departmentsPanel">
                <vbox id="departmentsTableBox">
